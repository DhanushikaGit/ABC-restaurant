    <style>
     :root {
            --primary: #FEA116;
            --light: #F1F8FF;
            --dark: #0F172B;
        }

       

        .navbar-dark .navbar-nav .nav-link {
            margin-left: 25px;
            padding: 35px 0;
            font-size: 15px;
            color: var(--light) !important;
            text-transform: uppercase;
            font-weight: 500;
            outline: none;
            transition: .5s;
        }

        .sticky-top.navbar-dark .navbar-nav .nav-link {
            padding: 20px 0;
        }

        .navbar-dark .navbar-nav .nav-link:hover,
        .navbar-dark .navbar-nav .nav-link.active {
            color: var(--primary) !important;
        }

        .navbar-dark .navbar-brand img {
            max-height: 60px;
            transition: .5s;
        }

        .sticky-top.navbar-dark .navbar-brand img {
            max-height: 45px;
        }

        @media (max-width: 991.98px) {
            .sticky-top.navbar-dark {
                position: relative;
            }

            .navbar-dark .navbar-collapse {
                margin-top: 15px;
                border-top: 1px solid rgba(255, 255, 255, .1);
            }

            .navbar-dark .navbar-nav .nav-link,
            .sticky-top.navbar-dark .navbar-nav .nav-link {
                padding: 10px 0;
                margin-left: 0;
            }

            .navbar-dark .navbar-brand img {
                max-height: 45px;
            }
        }

        @media (min-width: 992px) {
            .navbar-dark {
                position: absolute;
                width: 100%;
                top: 0;
                left: 0;
                z-index: 999;
                background: transparent !important;
            }

            .sticky-top.navbar-dark {
                position: fixed;
                background: var(--dark) !important;
            }
        }
        
     

        .btn-custom {
            background-color:#F1F8FF ;
            color: var(--light);
            border: 2px solid var(--primary);
        }

        .btn-custom:hover {
            background-color: var(--light);
            color: var(--primary);
            border-color: var(--primary);
        }

        .hero-wrap {
            background-size: cover;
            background-position: center;
            position: relative;
            height: 300px;
            display: flex;
            align-items: center;
            justify-content: center;
            color: var(--light);
        }

        .hero-wrap .overlay {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: rgba(0, 0, 0, 0.5);
        }

        .hero-wrap .text {
            position: relative;
            z-index: 1;
        }

        .text h1.bread {
            font-size: 48px;
            color: var(--primary);
        }

        .text p.breadcrumbs span a {
            color: var(--light);
        }

        .text p.breadcrumbs span {
            color: var(--primary);
        }

        .text-center h1 {
            color: var(--primary);
            font-size: 36px;
            margin-bottom: 20px;
        }

        .text-center p {
            color: var(--dark);
            font-size: 18px;
            margin-bottom: 20px;
        }
    </style>
</head>
